<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="net.togogo.mapper.UserMapper">
    <insert id="inertUser" parameterType="net.togogo.bean.User">
      INSERT  INTO  user(userId,userAccount,userPassword)VALUES (
      #{userId},
      #{userAccount},
      #{userPassword}
      )
    </insert>
</mapper>