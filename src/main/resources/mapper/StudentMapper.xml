<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="net.togogo.mapper.StudentMapper">

    <insert id="insertStudent" parameterType="net.togogo.bean.Student">

        INSERT  INTO  student (stuName,stuSex,stuAge)VALUES (

        #{stuName},
        #{stuSex},
        #{stuAge}
        )
    </insert>
</mapper>